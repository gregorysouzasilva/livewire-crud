{
    "name": "gregorysouzasilva/livewire-crud",
    "description": ":package_description",
    "license": "MIT",
    "authors": [
        {
            "name": "Gregory Souza Silva",
            "email": "author@email.com",
            "homepage": "http://author.com"
        }
    ],
    "homepage": "https://github.com/gregorysouzasilva/livewire-crud",
    "keywords": ["Laravel", "LivewireCrud"],
    "require": {
        "laravel/framework": "^10.0",
        "livewire/livewire": "^3.0.1",
        "spatie/laravel-permission": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~8.5"
    },
    "autoload": {
        "psr-4": {
            "Gregorysouzasilva\\LivewireCrud\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Gregorysouzasilva\\LivewireCrud\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gregorysouzasilva\\LivewireCrud\\LivewireCrudServiceProvider"
            ],
            "aliases": {
                "LivewireCrud": "Gregorysouzasilva\\LivewireCrud\\Facades\\LivewireCrud"
            }
        }
    }
}
